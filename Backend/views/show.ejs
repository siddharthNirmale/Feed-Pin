<% include ./partials/header.ejs %>

<div class="profile w-full min-h-screen bg-zinc-800 text-white">

  <div class="cards flex flex-wrap  pt-20  gap-5 px-10">
    <% user.posts.forEach(function(post) { %>
    <div class="w-32 card">

      <div class=" max-w-full max-h-40 bg-sky-200 rounded-xl overflow-hidden">
        <img src="/images/uploads/<%= post.image %>" alt="">
         
  
      </div>
      <h5 class="text-md font-medium opacity-50"><%= post.title %></h5>
    </div>
    <% }) %>

  </div>

</div>


<% include ./partials/footer.ejs %>