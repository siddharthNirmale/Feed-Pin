<% include ./partials/header.ejs %>

<div class="profile w-full min-h-screen bg-zinc-800 text-white">

  <div class="cards flex flex-wrap  pt-20  gap-5 px-10">
    <% posts.forEach(function(post) { %>
    <div class="w-32 card">

      <div class=" max-w-full max-h-40 bg-zinc-200 rounded-xl overflow-hidden">
        <img src="/images/uploads/<%= post.image %>" alt="">
         
  
      </div>
      <h5 class="text-md font-bold "><%= post.title %></h5>
      <div class="text-sm font-normal opacity-70">by <%= post.user.username %></div>
      

    </div>
    <% }) %>

  </div>

</div>


<% include ./partials/footer.ejs %>